	<update id="updatePublicEmpty">
		update parking_car_space sp
		LEFT JOIN
		parking_subscribe sb ON sb.car_space_id = sp.id
		set sp.is_empty = 1
		where (sb.start_time &gt; #{endTime}
		or sb.end_time &lt; #{startTime})
		and sp.is_empty = 0
	</update>

	<update id="updatePublicNoEmpty">
		update parking_car_space sp
		LEFT JOIN
		parking_subscribe sb ON sb.car_space_id = sp.id
		set sp.is_empty = 0
		where sb.start_time &lt; #{endTime}
		and sb.end_time &gt; #{startTime}
		and sp.is_empty = 1
